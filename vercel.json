{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/svg/:path*", "destination": "/SVG/:path*" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}
